<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <servlet>
        <servlet-name>FetchCampaigns</servlet-name>
        <servlet-class>DataSource.FetchCampaigns</servlet-class>
    </servlet>
   
    
    <servlet>
        <servlet-name>MDFRequest</servlet-name>
        <servlet-class>DataSource.MDFRequest</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>FetchCampaigns</servlet-name>
        <url-pattern>/FetchCampaigns</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>MDFRequest</servlet-name>
        <url-pattern>/MDFRequest</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ApplyCampaign</servlet-name>
        <servlet-class>DataSource.ApplyCampaign</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ApplyCampaign</servlet-name>
        <url-pattern>/ApplyCampaign</url-pattern>
    </servlet-mapping>

 
</web-app>
